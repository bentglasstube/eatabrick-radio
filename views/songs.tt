<% if songs.size > 0 %>
  <% for song in songs %>
    <div class="queue <% if current.file == song.file %>current<% end %>">
      <img src="/songs/<% song.album.uri %>.png" alt="cover" />
      <p><a href="/songs/<% song.uri %>"><% song.title %></a></p>
      <p><% song.artist %></p>
      <p><a href="/songs/<% song.album.uri %>"><% song.album.title %></a></p>
    </div>
  <% end %>
<% else %>
  <p class="empty">There are no matching songs.</p>
<% end %>
